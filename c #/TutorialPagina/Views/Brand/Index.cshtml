@model List<Brand>

<div class="container">
    <div class="row text-center">
        <h1>Marcas</h1>
    </div>
</div>

<table class="table">
    <thead>
        <tr>
            <th>Nombre</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(e => item.Name)</td>
            </tr>
        }
    </tbody>
</table>
